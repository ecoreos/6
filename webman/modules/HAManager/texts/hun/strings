remove	=	"Eltávolít"
shut_down	=	"Kikapcsolás"
sync	=	"Szinkronizálás"
unbind	=	"Lecsatlakozás"
update_dsm	=	"A _OSNAME_ frissítése"

[common]
processing	=	"Inicializálás..."

